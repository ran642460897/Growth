<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.growth">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:name=".core.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:fullBackupContent="true"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.jni.JniActivity"/>
        <activity android:name=".modules.gl.OpenGLActivity"/>
        <activity android:name=".modules.curl.CurlActivity"/>
        <activity android:name=".modules.spinner.SpinnerActivity"/>
        <activity android:name=".modules.smack.SmackActivity"/>
        <activity android:name=".modules.event.EventActivity"/>
    </application>

</manifest>
